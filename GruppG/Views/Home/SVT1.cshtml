@model IEnumerable<GruppG.Models.db.Program>

<p>
    @Html.ActionLink("Lägg till som favorit", "Create")
</p>
<table class="table">
    <tr>
        <th>
            <p>Program</p>
        </th>
        
        <th>
            <p>Kanal</p>
        </th>
        
        <th>
            <p>Datum & tid</p>
        </th>
        <th>
            <p>Datum & tid</p>
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Titel)
        </td>
        
        <td>
            @Html.DisplayFor(modelItem => item.Chanel)
        </td>
        
        <td>
            @Html.DisplayFor(modelItem => item.Starttime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Endtime)
        </td>
        <td>
            
            @Html.ActionLink("Mer info", "Details", new { id=item.Description })
            
        </td>
    </tr>
}

</table>
